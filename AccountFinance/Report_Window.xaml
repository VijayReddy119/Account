<Window x:Class="AccountFinance.Report_Window"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AccountFinance"
        xmlns:rsweb="clr-namespace:Microsoft.Reporting.WinForms;assembly=Microsoft.ReportViewer.WinForms"
        mc:Ignorable="d"
         Title="Report_Window" WindowState="Maximized">
    <Grid>
        <WindowsFormsHost>
            <rsweb:ReportViewer x:Name="Rpt"/>
        </WindowsFormsHost>
    </Grid>
</Window>
